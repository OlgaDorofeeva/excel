{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "linebreak-style": "off",
    "semi": "off",
    "camelcase": "off",
    "max-len": "off",
    "no-unused-expressions": "warn",
    "no-confusing-arrow": "off",
    "no-underscore-dangle": "off",
    "no-use-before-define": "off",
    "quotes": "off",
    "object-curly-spacing": "off",
    "require-jsdoc": "off",
    "arrow-parens": "off",
    "operator-linebreak": "off",
    "comma-dangle":"off"
  },
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
    "sourceType": "module"
  }
}
